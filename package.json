{
  "name": "react-broiler",
  "description": "Boilerplate for React based apps using Express.js, React-Bootstrap, MongoDB and Flux based architecture (via Alt) bundled through Webpack.",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/philspins/react-broiler"
  },
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "watch": "webpack --progress --colors --watch",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "alt": "^0.17.8",
    "async": "^1.5.0",
    "body-parser": "^1.14.1",
    "colors": "^1.1.2",
    "compression": "^1.6.0",
    "express": "^4.13.3",
    "history": "^1.13.0",
    "jquery": "^3.1.1",
    "mongoose": "^4.2.5",
    "morgan": "^1.6.1",
    "mysql": "^2.12.0",
    "react": "^0.14.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^0.14.2",
    "react-router": "^1.0.0",
    "react-router-bootstrap": "^0.23.1",
    "request": "^2.65.0",
    "serve-favicon": "^2.3.0",
    "socket.io": "^1.4.5",
    "swig": "^1.4.2",
    "underscore": "^1.8.3",
    "xml2js": "^0.4.15"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.1.18",
    "babel-register": "^6.3.13",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "license": "MIT"
}
